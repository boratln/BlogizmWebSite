
@{
	ViewData["Title"] = "Index";
	Layout = "~/Views/UILayout/Index.cshtml";
	var baslik = TempData["baslik"];
	var mesaj = TempData["mesaj"];
	var icon = TempData["icon"];
}
@if (TempData["mesaj"] != null)
{
	<script>
		Swal.fire({
			title:'@Html.Raw(baslik)',
			icon:'@Html.Raw(icon)',
			text:'@Html.Raw(mesaj)',
			confirmButtonText:'Tamam'
		})
	</script>
}
<div class="hero overlay inner-page bg-primary py-5">
	<div class="container">
		<div class="row align-items-center justify-content-center text-center pt-5">
			<div class="col-lg-6">
				<h1 class="heading text-white mb-3" data-aos="fade-up">Bizimle İletişime Geçin</h1>
			</div>
		</div>
	</div>
</div>

<div class="section">
	<div class="container">
		<div class="row">
			@await Component.InvokeAsync("_ContactAddressComponentPartial")
			@await Component.InvokeAsync("_ContactMessageComponentPartial")
		</div>
	</div>
</div> <!-- /.untree_co-section -->

